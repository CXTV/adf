		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "dl108lg_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"blb_input_connectionString": {
						"value": "[parameters('blb_input_connectionString')]"
					},
					"blb_output_connectionString": {
						"value": "[parameters('blb_output_connectionString')]"
					},
					"deleteBlob_connectionString": {
						"value": "[parameters('deleteBlob_connectionString')]"
					},
					"deleteFile_connectionString": {
						"value": "[parameters('deleteFile_connectionString')]"
					},
					"lg_blob_demolg01_connectionString": {
						"value": "[parameters('lg_blob_demolg01_connectionString')]"
					},
					"ls_db108lg_connectionString": {
						"value": "[parameters('ls_db108lg_connectionString')]"
					}
				}
			},
			"dependsOn": []
