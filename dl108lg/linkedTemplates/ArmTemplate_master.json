{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "dl108lg"
		},
		"blb_input_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'blb_input'"
		},
		"blb_output_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'blb_output'"
		},
		"deleteBlob_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'deleteBlob'"
		},
		"deleteFile_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'deleteFile'"
		},
		"lg_blob_demolg01_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'lg_blob_demolg01'"
		},
		"ls_db108lg_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'ls_db108lg'"
		},
		"trigger_by_fname_properties_pl_st_event_trigger_parameters_fname": {
			"type": "string",
			"defaultValue": "@triggerBody().fileName"
		},
		"trigger_by_fname_properties_pl_st_event_trigger_parameters_cname": {
			"type": "string",
			"defaultValue": "@triggerBody().folderPath"
		},
		"trigger_by_fname_properties_typeProperties_scope": {
			"type": "string",
