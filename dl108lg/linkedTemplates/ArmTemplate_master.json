					"trigger_by_fname_properties_pl_st_event_trigger_parameters_fname": {
						"value": "[parameters('trigger_by_fname_properties_pl_st_event_trigger_parameters_fname')]"
					},
					"trigger_by_fname_properties_pl_st_event_trigger_parameters_cname": {
						"value": "[parameters('trigger_by_fname_properties_pl_st_event_trigger_parameters_cname')]"
					},
					"trigger_by_fname_properties_typeProperties_scope": {
						"value": "[parameters('trigger_by_fname_properties_typeProperties_scope')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'dl108lg_ArmTemplate_0')]"
			]
		}
	]
