{
	"name": "pl12_josn2sql",
	"properties": {
		"activities": [
			{
				"name": "load json to sql",
				"type": "Copy",
				"dependsOn": [],
				"policy": {
					"timeout": "0.12:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"source": {
						"type": "JsonSource",
						"storeSettings": {
							"type": "AzureBlobStorageReadSettings",
							"recursive": true,
							"enablePartitionDiscovery": false
						},
						"formatSettings": {
							"type": "JsonReadSettings"
						}
					},
					"sink": {
						"type": "AzureSqlSink",
						"writeBehavior": "insert",
						"sqlWriterUseTableLock": false
					},
					"enableStaging": false,
					"translator": {
						"type": "TabularTranslator",
						"mappings": [
							{
								"source": {
									"path": "$['id']"
								},
								"sink": {
									"name": "empId",
									"type": "Int32"
								}
							},
							{
								"source": {
									"path": "$['name']"
								},
								"sink": {
									"name": "empName",
									"type": "String"
								}
							},
							{
								"source": {
									"path": "$['age']"
								},
								"sink": {
									"name": "age",
									"type": "Int32"
								}
							}
						]
					}
				},
				"inputs": [
					{
						"referenceName": "blb108lg_json",
						"type": "DatasetReference"
					}
				],
				"outputs": [
					{
						"referenceName": "AST_EmpInfo",
						"type": "DatasetReference"
					}
				]
			}
		],
		"annotations": []
	}
}